{
  "manifest_version": 3,
  "name": "Open Twitter",
  "description": "Open Twitter with Ctrl+Shift+S",
  "version": "1.0",
  "background": {
      "service_worker": "background.js"
  },
  "permissions": [
      "tabs"
  ],
  "commands": {
      "open-x-com": {
          "suggested_key": {
              "default": "Ctrl+Shift+S",
              "mac": "Command+Shift+S"
          },
          "description": "Open Twitter"
      }
  }
}
